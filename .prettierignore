# ignore the node_modules folder
node_modules/

# ignore the build or dist folder (depending on the project)
build/
dist/

# ignore project configuration files
*.config.js

# ignore the coverage folder generated by tests
coverage/

# ignore image files and other binaries
*.png
*.jpg
*.jpeg
*.gif
*.svg

# ignore log files
*.log

# ignore compiled CSS files
*.css

# ignore JSON files
*.json

# ignore YAML files
*.yaml
